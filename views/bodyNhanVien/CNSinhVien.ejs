
    <header id="wrap-header">
        <div class="container">
            <div class="row">
                <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12 logo">

                </div>
            </div>
            <div class="row">
                <h2 class="text-center">Cập nhật sinh viên

                </h2>
            </div>
            <div class="form-group col-xs-12 col-sm-12 col-md-12 col-lg-12" style="margin-top: 2%;">
                <a href="/cnsinhvien/add-sv" class="btn btn-primary" >Thêm sinh viên</a><br/>
                <div class="row" style="margin-top: 2%;">
                    <table class="table table-bordered" style="margin-top: 2%;">
                        <thead>
                            <tr>
                                <th>Mã sinh viên</th>
                                <th>Họ tên</th>
                                <th>Giới tính</th>
                                <th>Ngày sinh</th>
                                <th>Địa chỉ</th>
                                <th>Số điện thoại</th>
                                <th></th>
                            </tr>
                        </thead>
                        <tbody>
                            <%for(let i=0;i< listsv.length;i++){%>
                                <tr>
                                    <!-- <td><input type="radio"></td> -->
                                    <td><%=listsv[i].MSSV%></td>
                                    <td><%=listsv[i].HoTen  %></td>
                                    <td><%=listsv[i].GioiTinh%></td>
                                    <td><%=listsv[i].NgaySinh.getDate().toString()%>-<%=(listsv[i].NgaySinh.getMonth()+1).toString()%>-<%=listsv[i].NgaySinh.getFullYear().toString()%></td>
                                    <td><%=listsv[i].DiaChi%></td>
                                    <td><%=listsv[i].SoDT%></td>
                                    <td>
                                        <a href="/cnsinhvien/editsv/<%=listsv[i].MSSV%>" class="btn btn-warning" >Sửa</a>
                                        <a href="/cnsinhvien/deletesv/<%=listsv[i].MSSV%>" class="btn btn-danger " >Xóa</a>
                                    </td>
                                </tr>
                                <%}%>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </header>
