
    <header id="wrap-header">
        <div class="container" style="background-color: white;">
            <div class="row">  
                <h2 class="text-center">Cập nhật học kỳ

                </h2>
            </div>
            <div class="form-group col-xs-12 col-sm-12 col-md-12 col-lg-12" style="margin-top: 2%;">
              <ul class="nav nav-tabs">
                <li class="active"><a data-toggle="tab" href="#tab1">Danh sách học kỳ</a></li>
                <li><a data-toggle="tab" href="#tab2">Thêm học kỳ</a></li>
              </ul>
              <div class="tab-content">
                <div id="tab1" class="tab-pane fade in active">
                    <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12"> 
                        <div class="row">
                            <table class="table table-bordered" style="margin-top: 2%;">
                                <thead>
                                  <tr>
                                    <!-- <th>Chọn</th> -->
                                    <th>Học kỳ</th>
                                  </tr>
                                </thead>
                                <tbody>
                                  <%for(let i=0;i< listhocky.length;i++){%>
                                    <tr>
                                        <!-- <td><input type="radio"></td> -->
                                        <td><%=listhocky[i].HocKy%></td>
                                        <td>
                                            <a href="/nhanvien/cnhocky/deletehocky/<%=listhocky[i].HocKy%>" class="btn btn-danger" >Xóa</a>
                                        </td>
                                    </tr>
                                    <%}%>
                                </tbody>
                              </table>
                        </div>
                    </div>
                </div>
                <div id="tab2" class="tab-pane fade">
                </br>
                    <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                        <div class="col-xs-12 col-sm-12 col-md-12 col-lg-6">
                            <h4>Thêm Học Kỳ</h4>
                            <a href="/nhanvien/cnhocky/add-hocky" class="btn btn-primary" >Thêm học kỳ</a><br />
                        </div>
                    </div>
                </div>
              </div>  
            </div>
        </div>
    </header>
